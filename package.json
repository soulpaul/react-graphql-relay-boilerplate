{
  "name": "boilerplate-web-app",
  "version": "1.0.0",
  "description": "Boilerplate for a graphql/relay web client",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack-dev-server --config webpack/development.config.js",
    "build": "webpack --config webpack/production.config.js",
    "test": "jest",
    "relay": "relay-compiler --src . --schema ./schema.graphql --extensions js jsx ts tsx --artifactDirectory ./__generated__ --language typescript",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
